{
  "name": "stories-to-tests",
  "version": "0.1.12",
  "description": "generate stories tests files from `.stories.tsx`",
  "bin": "./dist/index.cjs",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "build": "tsup ./src/index.ts  --minify",
    "dev": "tsup ./src/index.ts --watch --sourcemap",
    "debug": " node --enable-source-maps --inspect ./dist/index.cjs",
    "check:tsc": "tsc --noEmit",
    "check:biome": "biome lint .",
    "check": "concurrently -c auto 'pnpm:check:*'",
    "format": "biome check --apply . ",
    "test": "vitest --run",
    "prepublish": "pnpm clear && pnpm build"
  },
  "keywords": [
    "storybook",
    "test",
    "cli"
  ],
  "author": "strozw",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.3",
    "rimraf": "^5.0.7",
    "tsup": "^8.0.2"
  },
  "dependencies": {
    "@storybook/core-common": "^8.1.4",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "eta": "^3.4.0",
    "glob": "^10.4.1",
    "valibot": "0.31.0-rc.7",
    "yargs": "^17.7.2"
  }
}
