{
  "name": "stories-to-tests",
  "version": "0.1.4",
  "description": "generate stories tests files from `.stories.tsx`",
  "bin": "./dist/index.cjs",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.1.3+sha256.7f63001edc077f1cff96cacba901f350796287a2800dfa83fe898f94183e4f5f",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup ./src/index.ts",
    "dev": "tsup ./src/index.ts --watch --sourcemap",
    "debug": " node --enable-source-maps --inspect ./dist/index.cjs ",
    "check:tsc": "tsc --noEmit",
    "check:biome": "biome lint .",
    "check": "concurrently -c auto 'pnpm:check:*'",
    "format": "biome check --apply . ",
		"postinstall": "lefthook install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@tsconfig/node-lts": "^20.1.3",
    "concurrently": "^8.2.2",
    "lefthook": "^1.6.14",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "valibot": "0.31.0-rc.7"
  },
  "dependencies": {
    "@storybook/core-common": "^8.1.4",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "eta": "^3.4.0",
    "glob": "^10.4.1",
    "yargs": "^17.7.2"
  }
}
